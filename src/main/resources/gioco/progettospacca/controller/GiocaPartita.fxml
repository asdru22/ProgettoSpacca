<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gioco.progettospacca.controller.HomeController">
   <children>
      <HBox alignment="CENTER" prefHeight="40.0">
         <children>
            <Label text="Inserisci codice partita">
               <font>
                  <Font name="Candara" size="12.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="200.0">
         <children>
            <TextField>
               <font>
                  <Font name="Candara" size="12.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="50.0">
         <children>
            <Button mnemonicParsing="false" text="Entra">
               <font>
                  <Font name="Candara" size="12.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
